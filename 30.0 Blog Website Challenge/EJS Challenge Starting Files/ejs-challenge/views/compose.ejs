<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="exampleFormControlInput1" class="form-label">Title</label>
        <input type="Title" name="blogTitle" class="form-control" id="exampleFormControlInput1">
      </div>
      <div class="form-group">
        <label for="exampleFormControlTextarea1" class="form-label">Post</label>
        <textarea type="text" class="form-control" id="exampleFormControlTextarea1" rows="3" name="blogPost"></textarea>
      </div>
      <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>


